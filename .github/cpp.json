{
	"problemMatcher": [
		{
			"owner": "gcc-clang",
			"pattern": [
				{
					"regexp": "^(\\x1b\\[[0-9;]*m)*(\\x1b\\[K)*([^:]+):.*(\\d+):(\\d+):(\\x1b\\[[0-9;]*m)*(\\x1b\\[K)*\\s+(?:fatal )?(\\x1b\\[[0-9;]*m)*(\\x1b\\[K)*(warning|error|note):\\s+(\\x1b\\[[0-9;]*m)*(\\x1b\\[K)*(.*)$",
					"file": 3,
					"line": 5,
					"column": 4,
					"severity": 10,
					"message": 13
				}
			]
		},
		{
			"owner": "msvc",
			"pattern": [
				{
					"regexp": "^(.*)\\((\\d+),?(\\d*)\\)\\s*:\\s*(error|warning|message)\\s+(\\w+\\s*:\\s+.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			]
		}
	]
}
