{
	"problemMatcher": [
		{
			"owner": "gcc-clang-linux",
			"pattern": [
				{
					"regexp": "^(\\/home\\/[^:]+):(\\d+):(\\d+):\\s+(?:fatal )?(warning|error|note):\\s+(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			]
		},
		{
			"owner": "gcc-clang-osx",
			"pattern": [
				{
					"regexp": "^(\\/Users\\/[^:]+):(\\d+):(\\d+):\\s+(?:fatal )?(warning|error|note):\\s+(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			]
		},
		{
			"owner": "msvc",
			"pattern": [
				{
					"regexp": "^(.*)\\((\\d+),?(\\d*)\\)\\s*:\\s*(error|warning|message)\\s+(\\w+\\s*:\\s+.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			]
		}
	]
}
